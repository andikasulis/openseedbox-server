
#Routes
GET		/public/											staticDir:public
GET		/status											Status.index
GET		/check											Check.index
GET		/backend/{action}\.{ext}					Backend.{action}
GET		/backend/{action}								Backend.{action}(ext:'json')
POST		/torrents/add									Torrents.add
GET		/torrents/{action}							Torrents.{action}
GET		/torrents/{hash}/{action}					Torrents.{action}
GET		/download/zipTest								Download.zipTest
GET		/download/{hash}								Download.downloadFile
GET		/													Application.index


